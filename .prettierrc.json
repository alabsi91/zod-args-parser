{
  "plugins": ["@trivago/prettier-plugin-sort-imports", "prettier-plugin-jsdoc"],
  "arrowParens": "avoid",
  "printWidth": 120,
  "semi": true,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "endOfLine": "lf",
  "singleQuote": false,
  "importOrder": ["<BUILTIN_MODULES>", "<THIRD_PARTY_MODULES>", "<SEPARATOR>", "^[./]", "<SEPARATOR>", "<TS_TYPES>"],
  "importOrderSeparation": true
}
